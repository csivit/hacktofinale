			<script type="text/javascript">
		document.getElementById("register").onclick = function()
			{

				regno = document.getElementById("inputEName").value
				pass = document.getElementById("inputPass").value
				username = document.getElementById("inputName").value
				usermail = document.getElementById("inputEmail").value
				usermob = document.getElementById("inputMob").value
				usertext1 = document.getElementById("textArea").value
				usertext2 = document.getElementById("textArea1").value
			
			
				validregno = /(15)([a-zA-Z][a-zA-Z][a-zA-Z])(\d\d\d\d)/;
				validemail = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
				validphone = /^[789]\d{9}$/; /*10 digit mobile number starting with 7/8/9.*/

			
			if (!validregno.test(regno))
				{
					alert("Enter A Valid Registration Number - First Years Only.")
				}
			else if (!validphone.test(usermob))
				{
					alert("Enter your 10 digit Phone Number.")
				}
			else if (!validemail.test(usermail))
				{
					alert("Enter A Valid Email ID.")
				}
			else if (username == '')
				{
					alert("Enter your Name.")
				}
			else if (pass == '')
				{
					alert("Enter a Password.")
				}
			else if (usertext1 == '')
				{
					alert("Hey! Tell us something about yourself?")
				}
			else if (usertext2 == '')
				{
					alert("Are you a team player or a team leader?")
				}
			else {
		document.getElementById("cForm").submit()
			}
		}
		
		document.getElementById("login").onclick = function()
			{

				regnor = document.getElementById("inputENameL").value
				passl = document.getElementById("inputPassL").value
			
				validregno = /(15)([a-zA-Z][a-zA-Z][a-zA-Z])(\d\d\d\d)/; 

			
			if (!validregno.test(regnor))
				{
					alert("Enter A Valid Registration Number - First Years Only.")
				}
			else if (passl == '')
				{
					alert("Enter a Password.")
				}
			else {
						document.getElementById("cFormL").submit() 
			}
		}
